<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Action Height="1.4" ID="TakeOff"/>
            <Action ID="SetLocation" Name="home" Pos="1,2" name="home"/>
            <Action ID="SetLocation" Name="charger" Pos="5,5" name="charger"/>
            <Action ID="GoTo" name="drone1" target="charger"/>
            <Action ID="GoTo" name="drone1" target="home"/>
            <Action ID="Land" name=""/>
            <Action Height="1" ID="TakeOff"/>
            <Action ID="Land"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="GoTo">
            <input_port name="target" type="std::string"/>
        </Action>
        <Action ID="Land"/>
        <Action ID="SetLocation">
            <input_port name="Name"/>
            <input_port name="Pos"/>
        </Action>
        <Action ID="TakeOff">
            <input_port name="Height"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

